@{
    ViewBag.Title = "Fiabilidad";
}

<div class="row">
    <h1>Fiabilidad</h1>
    <div class="row">
        <table class="table table-responsive">
            <tr>
                <th>Categorías</th>
                <th>Pésimo</th>
                <th>Mal</th>
                <th>Regular</th>
                <th>Bueno</th>
                <th>Excelente</th>
            </tr>
            @foreach (var fiabilidad in Model)
            {
                <tr>
                    <td>@fiabilidad.nombre</td>
                    <td>@fiabilidad.f1</td>
                    <td>@fiabilidad.f2</td>
                    <td>@fiabilidad.f3</td>
                    <td>@fiabilidad.f4</td>
                    <td>@fiabilidad.f5</td>
                </tr>
            }
        </table>
        <div style="display: flex; align-items: center; justify-content: center; margin: 20px 0;">
            <iframe width="1500" height="800" src="https://app.powerbi.com/view?r=eyJrIjoiMTNlMzNhMzAtZTY5Ny00OTA0LWE3YTAtMDQ5NTY3Zjk2OGIzIiwidCI6ImY0MmM0NzEyLTY2Y2EtNGY4My1iMTg4LTQ3ZDE2NTNiNmU3MCJ9" frameborder="0" allowFullScreen="true"></iframe>
        </div>
        <div>
            <input class="btn btn-primary" type="button" onclick="history.back()" name="volver atrás" value="Volver">
        </div>
    </div>
</div>